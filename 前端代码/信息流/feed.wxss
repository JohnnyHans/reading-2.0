/* pages/feed/feed.wxss */
.container{
  width: 100%;
  height: 100%;
  padding-top: 20rpx;
  padding-bottom: 26rpx;
  display: flex;
  flex-direction: column;
  align-items: center;
  background-color: rgba(206, 206, 206, 0.158);
}

.feed{
  display: flex;
  text-align: justify;
  align-items: center;
  margin-top: 20rpx;
  padding-left: 40rpx;
  padding-right: 40rpx;
  font-size: 32rpx;
  font-family:  'Franklin Gothic Medium', 'Arial Narrow', Arial, sans-serif;
  color: #535353;
  width: 96%;
  height: 162rpx;
  background: white;
  border: 1rpx solid rgba(221, 221, 221, 0.616);
  box-shadow: 1rpx 1rpx 2rpx rgba(212, 212, 212, 0.753);
  border-radius: 12rpx;
}
.feed1{
  position: relative;
  width: 700rpx;
  height: 164rpx;
  margin-top: 10rpx;
}